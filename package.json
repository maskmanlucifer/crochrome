{
  "name": "crochrome",
  "version": "1.0.0",
  "homepage": "https://maskmanlucifer.github.io/crochrome",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "cp -r build/* public/ 2>/dev/null || true && touch public/.nojekyll",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public"
