  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-crop": "^10.1.8"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "react-scripts": "5.0.1"
  }
}
